<div class="container">
  <div class="row">
    <%= form_for(:session, url: login_path, html: { class: "col s12" }) do |f| %>

    <div id="log-in" class="col s12 m6 l6 offset-m3 offset-l3">
      <h4 class="row center-align">Log in</h4>
      <div class="row">

      <div class="input-field col s12">
        <i class="material-icons prefix">email</i>
        <%= f.label :email, "Email" %>
        <%= f.email_field :email %>
      </div>
       <div class="input-field col s12">
        <i class="material-icons prefix">security</i>
        <%= f.label :password, "Password" %>
        <%= f.password_field :password %>
      </div>
      <button class="btn waves-effect waves-light col s12 m4 l4 offset-m4 offset-l4" type="submit">Log In
        <i class="material-icons right">done</i>
      </button>
      </div>
    </div>
    <% end %>
  </div>
</div>
